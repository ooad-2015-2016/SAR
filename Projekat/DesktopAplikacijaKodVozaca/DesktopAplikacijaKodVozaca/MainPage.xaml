        <Button x:Name="button_Copy1" Content="Button" HorizontalAlignment="Left" Margin="512,10,0,0" VerticalAlignment="Top" Height="79" Width="253"/>
        <Button x:Name="button_Copy2" Content="Button" HorizontalAlignment="Left" Margin="770,10,0,0" VerticalAlignment="Top" Height="79" Width="254"/>
        <ListBox x:Name="listBox" HorizontalAlignment="Left" Height="529" Margin="10,217,0,0" VerticalAlignment="Top" Width="225"/>
