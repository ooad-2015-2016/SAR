<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>

   

    <link rel="icon" href="@Url.Content("~/Content/MyTemplate/")images/favicon.ico">
    <link rel="shortcut icon" href="@Url.Content("~/Content/MyTemplate/")images/favicon.ico" />
    <link rel="stylesheet" href="@Url.Content("~/Content/MyTemplate/")booking/css/booking.css">
    <link rel="stylesheet" href="@Url.Content("~/Content/MyTemplate/")css/camera.css">
    <link rel="stylesheet" href="@Url.Content("~/Content/MyTemplate/")css/owl.carousel.css">
    <link rel="stylesheet" href="@Url.Content("~/Content/MyTemplate/")css/style.css">
    <script src="@Url.Content("~/Content/MyTemplate/")js/jquery.js"></script>
    <script src="@Url.Content("~/Content/MyTemplate/")js/jquery-migrate-1.2.1.js"></script>
    <script src="@Url.Content("~/Content/MyTemplate/")js/script.js"></script>
    <script src="@Url.Content("~/Content/MyTemplate/")js/superfish.js"></script>
    <script src="@Url.Content("~/Content/MyTemplate/")js/jquery.ui.totop.js"></script>
    <script src="@Url.Content("~/Content/MyTemplate/")js/jquery.equalheights.js"></script>
    <script src="@Url.Content("~/Content/MyTemplate/")js/jquery.mobilemenu.js"></script>
    <script src="@Url.Content("~/Content/MyTemplate/")js/jquery.easing.1.3.js"></script>
    <script src="@Url.Content("~/Content/MyTemplate/")js/owl.carousel.js"></script>
    <script src="@Url.Content("~/Content/MyTemplate/")js/camera.js"></script>
    <!--[if (gt IE 9)|!(IE)]><!-->
    <script src="@Url.Content("~/Content/MyTemplate/")js/jquery.mobile.customized.min.js"></script>
    <!--<![endif]-->
    <script src="@Url.Content("~/Content/MyTemplate/")booking/js/booking.js"></script>
    <script>
			$(document).ready(function(){
				jQuery('#camera_wrap').camera({
					loader: false,
					pagination: false ,
					minHeight: '444',
					thumbnails: false,
					height: '28.28125%',
					caption: true,
					navigation: true,
					fx: 'mosaic'
				});
				$().UItoTop({ easingType: 'easeOutQuart' });
			});
    </script>
    <!--[if lt IE 8]>
        <div style=' clear: both; text-align:center; position: relative;'>
