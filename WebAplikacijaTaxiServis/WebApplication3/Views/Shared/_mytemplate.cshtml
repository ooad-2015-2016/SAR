</head>
<body>
    </head>
    <body class="page1" id="top">
        <div class="main">
            <!--==============================header=================================-->
            <header>
                <div class="menu_block ">
                    <div class="container_12">
                        <div class="grid_12">
                            <nav class="horizontal-nav full-width horizontalNav-notprocessed">
                                <ul class="sf-menu">
                                    <li class="current"><a href="index.html">Home</a></li>
                                    <li><a href="index-1.html">O Nama</a></li>
                                    <li><a href="@Url.Content("~/Rezervacija1/")Create">Rezervacija</a></li>
                                    <li><a href="index-3.html">Naručivanje</a></li>
                                    <li><a href="index-4.html">Kontakt</a></li>
                                </ul>
                            </nav>
                            <div class="clear"></div>
                        </div>
                        <div class="clear"></div>
                    </div>
                </div>
                <div class="container_12">
                    <div class="grid_12">
                        <h1>
                            <a href="index.html">
                                <img src="images/logo.png" alt="Your Happy Family">
                            </a>
                        </h1>
                    </div>
                </div>
                <div class="clear"></div>
            </header>
            <div class="slider_wrapper ">
                <div id="camera_wrap" class="">
                    <div data-src="images/slide.jpg"></div>
                    <div data-src="images/slide1.jpg"></div>
                    <div data-src="images/slide2.jpg"></div>
                </div>
            </div>
            <div class="container_12">
                <div class="grid_4">
                    <div class="banner">
                        <div class="maxheight">
                            <div class="banner_title">
                                <img src="images/icon1.png" alt="">
                                <div class="extra_wrapper">
                                    Brzo
                                    <div class="color1">Pouzdano</div>
                                </div>
                            </div>
                            <a href="#" class="fa fa-share-square"></a>
                        </div>
                    </div>
                </div>
                <div class="grid_4">
                    <div class="banner">
                        <div class="maxheight">
                            <div class="banner_title">
                                <img src="images/icon2.png" alt="">
                                <div class="extra_wrapper">
                                    Najbolja
                                    <div class="color1">Cijena</div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="grid_4">
                    <div class="banner">
                        <div class="maxheight">
                            <div class="banner_title">
                                <img src="images/icon3.png" alt="">
                                <div class="extra_wrapper">
                                    Jedan
                                    <div class="color1">Klik</div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="clear"></div>
            </div>
            <div class="c_phone">
                <div class="container_12">
                    <div class="grid_12">
                        <div class="fa fa-phone"></div>
                        + 1800 559 6580
                        <span>Naručite i telefonom!</span>
                    </div>
                    <div class="clear"></div>
                </div>
            </div>
            <div>
                @RenderBody()
            </div>

    </body>
</html>
